        <attr name="tvLeftTitle" format="string"/>
        <attr name="tvTitle" format="string"/>
        <attr name="tvRightTitle" format="string"/>

        <attr name="tvTitleColor" format="color"/>
        <attr name="tvLeftTitleColor" format="color"/>
        <attr name="tvRightTitleColor" format="color"/>

        <attr name="tvLeftTitleSize" format="dimension"/>
